{
  "expo": {
    "name": "highline-app",
    "slug": "highline-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
